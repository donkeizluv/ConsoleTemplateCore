{
  "NLog": {
    "targets": {
      "logconsole": {
        "type": "ColoredConsole",
        "layout": "${longdate}|${level:uppercase=true}|${message}",
        "useDefaultRowHighlightingRules": true
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "logconsole"
      }
    ]
  }
}